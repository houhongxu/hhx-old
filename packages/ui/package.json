{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsup src/index.tsx --format esm,cjs  --dts --legacy-output --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --legacy-output --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\""
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "medium-zoom": "^1.0.6",
    "react-blurhash": "^0.1.3",
    "classnames": "^2.3.1",
    "query-string": "^7.1.1",
    "ahooks": "3.5.2",
    "zustand": "3.7.2",
    "antd": "^4.18.5",
    "@ant-design/icons": "^4.7.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.1",
    "eslint": "^7.32.0",
    "typescript": "^4.5.2",
    "tsup": "^6.1.3",
    "tailwindcss": "^3.1.5",
    "tailwind-config": "workspace:*",
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "helper":"workspace:*"
  }
}
